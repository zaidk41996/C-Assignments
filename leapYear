#include <iostream>

using namespace std;

int main()
{
    int leap_year;

    cout << "Enter a Year: ";
    cin >> leap_year;

    if ((leap_year % 4 == 0 && leap_year % 100 != 0) || ( leap_year % 400 == 0))
    {
        cout << leap_year << " is a Leap Year" << endl;
    }
    else
    {
        cout << leap_year << " is a not a Leap Year" << endl;
    }
}
